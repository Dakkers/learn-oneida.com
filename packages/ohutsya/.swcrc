{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "jsx": true,
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic", // or "classic"
        "refresh": true // Only for development
      }
    },
    "target": "es2020" // or "es5", "es2015", etc.
  },
  "module": {
    "type": "es6" // or "commonjs"
  }
}
